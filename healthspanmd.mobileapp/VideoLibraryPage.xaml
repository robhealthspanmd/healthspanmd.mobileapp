<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="healthspanmd.mobileapp.VideoLibraryPage">
    <StackLayout>
        <ListView x:Name="VideoList" ItemSelected="OnVideoSelected">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding}" />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
        <WebView x:Name="YouTubeWebView" HeightRequest="300" WidthRequest="400" />
    </StackLayout>
</ContentPage>